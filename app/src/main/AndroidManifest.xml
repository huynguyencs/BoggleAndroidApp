<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymac.boggle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameDifficulty" />
        <activity android:name=".MainBoard" />
        <activity android:name=".MultiplayerMode" />
        <activity android:name=".Instruction" />
        <activity android:name=".Leaderboard" />
        <activity android:name=".Results" />
        <activity android:name=".BluetoothManager"></activity>
    </application>

</manifest>